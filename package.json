{
  "name": "katapult",
  "version": "0.30.0",
  "description": "Build automation tool by launchdeck.io",
  "main": "bin/katapult",
  "bin": {
    "katapult": "bin/katapult.js"
  },
  "command_root": "bin",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "pretest": "jshint lib",
    "depcheck": "depcheck --ignore-bin-package=false",
    "test": "mocha --recursive -R spec",
    "test-coverage": "istanbul cover ./node_modules/mocha/bin/_mocha -- --recursive && opn coverage/lcov-report/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/katapult/katapult.git"
  },
  "keywords": [
    "build",
    "deployment",
    "automation",
    "tool"
  ],
  "author": "JM Versteeg",
  "license": "AGPL-3.0",
  "homepage": "https://github.com/launchdeckio/katapult",
  "bugs": {
    "url": "https://github.com/launchdeckio/katapult/issues"
  },
  "dependencies": {
    "bluebird": "^3.3.4",
    "chalk": "^1.1.3",
    "defa": "0.1.0",
    "del": "^2.2.0",
    "human-interval": "^0.1.5",
    "inquirer": "^0.12.0",
    "is-glob-blacklist": "0.0.1",
    "johnnycache": "^1.0.0",
    "js-yaml": "^3.5.5",
    "lodash": "^4.6.1",
    "minimatch": "^3.0.0",
    "mkdirp": "^0.5.1",
    "mock-fs": "^3.8.0",
    "node-notifier": "^4.5.0",
    "numeral": "^1.5.3",
    "pretty-error": "^2.0.0",
    "q-io": "^1.13.2",
    "slug": "^0.9.1",
    "sprintf-js": "^1.0.3",
    "stream-buffers": "^3.0.0",
    "update-notifier": "^0.6.3",
    "worq": "^2.1.2",
    "yargs": "^4.3.2"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "coveralls": "^2.11.9",
    "eslint": "^2.5.3",
    "istanbul": "^0.4.2",
    "jshint": "^2.9.1",
    "mocha": "^2.4.5",
    "opn-cli": "^3.1.0",
    "rewire": "^2.5.1",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0"
  },
  "preferGlobal": true
}
